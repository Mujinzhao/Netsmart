<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.0</tlib-version>
 <jsp-version>1.2</jsp-version>
 <short-name>mytable</short-name>
 <uri>/mytable</uri>
 <tag>
  <name>mytable</name>
  <tag-class>com.core.tag.TableTag</tag-class>
  <body-content>JSP</body-content>
  <attribute>
   <name>tableName</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>headCols</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>colPK</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>isRowNum</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>selectRowBtn</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>hidCols</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>sqlKey</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>sqlParam</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>isPage</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>pageSize</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>expBtn</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>isSort</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>firstShow</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>dsName</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>actionCol</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>maxLength</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>sqlScope</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>reqSortCols</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>sortVal</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
 <tag>
  <name>myparam</name>
  <tag-class>com.core.tag.TableParam</tag-class>
  <body-content>JSP</body-content>
  <attribute>
   <name>id</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>dsName</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>resId</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
  <attribute>
   <name>tabId</name>
   <rtexprvalue>true</rtexprvalue>
  </attribute>
 </tag>
</taglib>
